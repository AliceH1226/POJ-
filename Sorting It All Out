//https://vjudge.net/problem/POJ-1094
//https://blog.csdn.net/sdjzujxc/article/details/8634316






#include<iostream>
#include<stdio.h>
#include<string.h>

using namespace std;

int mp[27][27],q[27],dg[27];
int Toposort(int n)
{
    int c=0,tmp[27],loc,m,flag=1;
    for(int i=1;i<=n;i++)
    {
        tmp[i]=dg[i];
    }
    for(int i=1;i<=n;i++)
    {
        m=0;
        for(int j=1;j<=n;j++)
        {
            if(tmp[j]==0)
            {
                m++;
                loc=j;

            }
        }
        if(m==0)return 0;
        if(m>1)flag=-1;
        q[c++]=loc;
        tmp[loc]=-1;
        for(int j=1;j<=n;j++)
        {
            if(mp[loc][j]==1)
                tmp[j]--;

        }
    }
    return flag;
}

int main()
{
    int m,n,flag;
    char b[5];
    while(cin>>m>>n)
    {
        memset(mp,0,sizeof(mp));
        memset(dg,0,sizeof(dg));
        if(m==0&&n==0)break;
        flag=0;
        for(int i=1;i<=n;i++)
        {
            scanf("%s",b);
            if(flag)continue;
            int x=b[0]-'A'+1;
            int y=b[2]-'A'+1;
            mp[x][y]=1;
            dg[y]++;
            int s=Toposort(m);
            if(s==0)
            {
                cout<<"Inconsistency found after "<<i<<" relations."<<endl;
                flag=1;
            }
            if(s==1)
            {
                cout<<"Sorted sequence determined after "<<i<<" relations: ";
                for(int j=0;j<m;j++)
                {
                    printf("%c",q[j]-1+'A');
                }
                cout<<"."<<endl;
                flag=1;

            }
        }
        if(!flag)
        {
            cout<<"Sorted sequence cannot be determined."<<endl;
        }
    }
}
