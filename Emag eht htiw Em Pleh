//https://blog.csdn.net/qq_37605573/article/details/75647852
//http://poj.org/problem?id=2993


#include<stdio.h>
#include<string.h>

int main()
{
    char map[][50]={
"+---+---+---+---+---+---+---+---+",
"|...|:::|...|:::|...|:::|...|:::|",
"+---+---+---+---+---+---+---+---+",
"|:::|...|:::|...|:::|...|:::|...|",
"+---+---+---+---+---+---+---+---+",
"|...|:::|...|:::|...|:::|...|:::|",
"+---+---+---+---+---+---+---+---+",
"|:::|...|:::|...|:::|...|:::|...|",
"+---+---+---+---+---+---+---+---+",
"|...|:::|...|:::|...|:::|...|:::|",
"+---+---+---+---+---+---+---+---+",
"|:::|...|:::|...|:::|...|:::|...|",
"+---+---+---+---+---+---+---+---+",
"|...|:::|...|:::|...|:::|...|:::|",
"+---+---+---+---+---+---+---+---+",
"|:::|...|:::|...|:::|...|:::|...|",
"+---+---+---+---+---+---+---+---+"
    };
    char a[100];
    gets(a);
    for(int i=0;a[i]!='\0';i++)
    {
        if(a[i]==' '||a[i]==',')
        {
            if(a[i+1]=='a'||a[i+1]=='b'||a[i+1]=='c'||a[i+1]=='d'||a[i+1]=='e'||a[i+1]=='f'||a[i+1]=='g'||a[i+1]=='h')
                map[(8-(a[i+2]-'0'))*2+1][4*(a[i+1]-'a'+1)-1-1]='P';
            else
                map[(8-(a[i+3]-'0'))*2+1][4*(a[i+2]-'a'+1)-1-1]=a[i+1];
        }
    }
    gets(a);
    for(int i=0;a[i]!='\0';i++)
    {
        if(a[i]==' '||a[i]==',')
        {
            if(a[i+1]=='a'||a[i+1]=='b'||a[i+1]=='c'||a[i+1]=='d'||a[i+1]=='e'||a[i+1]=='f'||a[i+1]=='g'||a[i+1]=='h')
                map[(8-(a[i+2]-'0'))*2+1][4*(a[i+1]-'a'+1)-1-1]='p';
            else
                map[(8-(a[i+3]-'0'))*2+1][4*(a[i+2]-'a'+1)-1-1]=a[i+1]+32;
        }
    }
    for(int i=0;i<17;i++)
    {
        puts(map[i]);
    }
}
