//http://www.voidcn.com/article/p-avxotlwe-bgk.html

#include<iostream>
#include<stdio.h>
#include<stdlib.h>


int main()
{
    long long ans,s,d;
    while(~scanf("%lld%lld",&s,&d))
    {
        if(d>s*4)
        {
            ans=10*s-2*d;

        }
        else if(2*d>3*s)
        {
            ans=8*s-4*d;
        }
        else if(3*d>2*s)
        {
            ans=6*s-6*d;
        }
        else if(4*d>s)
        {
            ans=3*s-9*d;
        }
        else ans=-1;
        if(ans>=0)
        {
            printf("%lld\n",ans);
        }
        else
        {
            printf("Deficit\n");
        }
    }

}
